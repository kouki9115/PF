<main><h1>Blogs#index</h1>
<p>Find me in app/views/blogs/index.html.erb</p>

<p><%=link_to "blog#new", new_blog_path %></p>

<% @blogs.each do |blog| %>

 <p4><%=attachment_image_tag blog, :image, :fill, 100, 100, format: 'jpeg' %></p4>
 <p4><%=blog.name %></p4>
 <p4><%=blog.price %></p4>
 <p4><%=blog.title %></p4>
 <p4><%=blog.body %></p4>
 <p4><%= link_to "詳細", blog_path(blog) %>
 <%= link_to "削除", blog_path(blog), method: :delete%></p4>
<% end %>

</main>